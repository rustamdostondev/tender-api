    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.5.0",
    "llmwhisperer-client": "^2.3.0",
    "multer": "^1.4.5-lts.2",
    "nestjs-minio-client": "^2.2.0",
    "openai": "^4.100.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
